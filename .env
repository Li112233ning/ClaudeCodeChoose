# 开发时不自动打开浏览器
BROWSER=none

# 生产构建时不生成 source map（减小文件大小）
GENERATE_SOURCEMAP=false

# Electron 开发模式标识
ELECTRON_IS_DEV=false

# React 应用配置
REACT_APP_VERSION=1.0.0
REACT_APP_NAME=claude-key-manager

# 自定义端口（默认 3000）
PORT=3000

# Claude Code 相关环境变量
REACT_APP_CLAUDE_API_BASE_URL=https://api.anthropic.com
REACT_APP_CLAUDE_MODEL_DEFAULT=claude-4-sonnet
REACT_APP_MAX_TOKENS_DEFAULT=4096
REACT_APP_TEMPERATURE_DEFAULT=0.7

# 应用配置
REACT_APP_APP_TITLE=Claude Key Manager
REACT_APP_DEBUG_MODE=false
REACT_APP_ENCRYPTION_ENABLED=true

# 开发环境特定配置
NODE_ENV=development
REACT_APP_ENV=development
